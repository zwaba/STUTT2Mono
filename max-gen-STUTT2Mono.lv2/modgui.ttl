@prefix modgui: <http://moddevices.com/ns/modgui#> .
@prefix lv2:    <http://lv2plug.in/ns/lv2core#> .

<urn:maxgen:STUTT2Mono>
    modgui:gui [
        modgui:resourcesDirectory <modgui> ;
        modgui:iconTemplate <modgui/icon-stutt2mono.html> ;
        modgui:stylesheet <modgui/stylesheet-stutt2mono.css> ;
        modgui:screenshot <modgui/screenshot-stutt2mono.png> ;
        modgui:thumbnail <modgui/thumbnail-stutt2mono.png> ;
        modgui:brand "zwabo" ;
        modgui:label "STUTT2Mono" ;
        modgui:model "boxy" ;
        modgui:panel "6-knobs" ;
        modgui:color "white" ;
        modgui:knob "bronze" ;
        modgui:port [
            lv2:index 0 ;
            lv2:symbol "BPM" ;
            lv2:name "BPM" ;
        ] , [
            lv2:index 1 ;
            lv2:symbol "Div_L" ;
            lv2:name "Div_L" ;
        ] , [
            lv2:index 2 ;
            lv2:symbol "ENV_ms" ;
            lv2:name "ENV_ms" ;
        ] , [
            lv2:index 3 ;
            lv2:symbol "speed" ;
            lv2:name "speed" ;
        ] , [
            lv2:index 4 ;
            lv2:symbol "RANGE" ;
            lv2:name "RANGE" ;
        ] , [
            lv2:index 5 ;
            lv2:symbol "dry_wet" ;
            lv2:name "dry_wet" ;
        ] ;
    ] .